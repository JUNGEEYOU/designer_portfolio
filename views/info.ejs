<!DOCTYPE html>
<html ng-app="scotchApp">
<head>
    <!-- SPELLS -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="javascripts/jquery.cycle.all.min.js"></script>
    <style type="text/css">
        .pics { height: 100%; width: 100%; margin: auto }
        .pics img { padding: 0; border: 0; background-color: #000; }
        #avaf { cursor: pointer }
    </style>
    <script type="text/javascript">
        $(document).ready(function() {
            $('#thek').cycle({
                fx:      'scrollLeft',
                next:   '#thek',
                timeout:  0
            });
        });
    </script>
    <link rel="stylesheet" type="text/css" href="stylesheets/dots.css" media="screen"/>
    <link rel="stylesheet" type="text/css" href="stylesheets/copywhite.css" media="screen"/>
    <link rel="stylesheet" type="text/css" href="stylesheets/scalerw.css" media="screen"/>
</head>
<body>
<div style="padding:0;background:transparent;position:fixed;left:30px;top:50px;z-index:99;cursor:default">
    <div class="no">
        <a href="/">0</a><br/>
        <a href="pre" >1</a><br/>
        <a href="info" class="act">2</a><br/>
    </div>
</div>
<!-- define angular controller -->
<div id="bg">
    <div id="thek" class="pics">
        <b><i><i><img alt="" src="/images/background/1.jpg"/></i></i></b>
        <b><i><i><img alt="" src="/images/background/2.jpg"/></i></i></b>
    </div>
</div>
</body>
</html>